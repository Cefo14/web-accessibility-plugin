{
  "name": "web-accessibility-plugin",
  "version": "1.0.0",
  "description": "A simple web accessibility plugin for browsers",
  "author": "Cefo14 <cefo14@protonmail.com>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cefo14/web-accessibility-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Cefo14/web-accessibility-plugin/issues"
  },
  "homepage": "https://github.com/Cefo14/web-accessibility-plugin#readme",
  "keywords": [
    "open source",
