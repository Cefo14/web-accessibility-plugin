    "javascript",
    "typescript",
    "vite"
  ],
  "main": "dist/react-web-accessibility-plugin.cjs.js",
  "module": "dist/react-web-accessibility-plugin.es.js",
  "types": "dist/react-web-accessibility-plugin.es.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:module": "tsc -b && vite build --config vite.module.config.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "commit": "cz",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
     "typecheck": "tsc --noEmit"
  },
